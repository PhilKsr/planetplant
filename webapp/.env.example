# Production Environment Variables
# Copy this file to .env and fill in your actual values

# InfluxDB Configuration
INFLUXDB_USERNAME=admin
INFLUXDB_PASSWORD=your_secure_password
INFLUXDB_ORG=planetplant
INFLUXDB_BUCKET=sensors
INFLUXDB_TOKEN=your_influxdb_token

# MQTT Configuration
MQTT_USERNAME=planetplant
MQTT_PASSWORD=your_mqtt_password

# Redis Configuration
REDIS_PASSWORD=your_redis_password

# SSL Certificate Email (for Let's Encrypt)
ACME_EMAIL=your_email@example.com

# PWA Push Notifications (VAPID)
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key
VITE_VAPID_PRIVATE_KEY=your_vapid_private_key

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_WS_URL=ws://localhost:3001

# App Configuration
VITE_APP_NAME=PlanetPlant
VITE_APP_VERSION=1.0.0
VITE_BUILD_HASH=dev