{
  "name": "planetplant-webapp",
  "version": "1.0.0",
  "description": "Smart Plant Watering System - React PWA Frontend",
  "homepage": "./",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "pwa:generate-icons": "pwa-asset-generator public/logo.svg public/icons --favicon --apple --manifest public/manifest.json",
    "serve": "serve -s build -l 3000",
    "deploy": "npm run build && npm run serve"
  },
  "dependencies": {
    "@headlessui/react": "2.1.8",
    "@hookform/resolvers": "3.9.0",
    "@tabler/icons-react": "^3.14.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tanstack/react-query": "5.56.2",
    "@tanstack/react-query-devtools": "5.56.2",
    "axios": "1.7.7",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "framer-motion": "11.5.4",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.542.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "7.53.0",
    "react-hot-toast": "2.4.1",
    "react-i18next": "^15.7.3",
    "react-icons": "5.3.0",
    "react-intersection-observer": "9.13.1",
    "react-router-dom": "6.26.2",
    "recharts": "2.12.7",
    "socket.io-client": "4.8.0",
    "tailwind-merge": "2.5.2",
    "use-debounce": "10.0.3",
    "workbox-precaching": "7.1.0",
    "workbox-routing": "7.1.0",
    "workbox-strategies": "7.1.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/ui": "2.0.5",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-react": "7.35.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "jsdom": "25.0.0",
    "postcss": "8.4.45",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "serve": "14.2.3",
    "tailwindcss": "3.4.10",
    "typescript": "5.6.2",
    "vite": "5.4.6",
    "vite-plugin-pwa": "0.20.5",
    "vitest": "2.0.5"
  },
  "keywords": [
    "react",
    "pwa",
    "plant-watering",
    "iot",
    "dashboard",
    "monitoring",
    "responsive"
  ],
  "author": "PlanetPlant Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/PlanetPlant.git",
    "directory": "webapp"
  },
  "bugs": {
    "url": "https://github.com/yourusername/PlanetPlant/issues"
  }
}
