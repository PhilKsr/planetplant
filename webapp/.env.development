# Development Environment Variables for Frontend
# Used by Vite during development

# API Configuration - nginx proxy for Docker development
VITE_API_URL=http://localhost:8080/api

# WebSocket Configuration - nginx proxy for Docker development  
VITE_WS_URL=http://localhost:8080

# Feature Flags
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_PWA_DEBUG=true
VITE_LOG_LEVEL=debug

# Development Options
VITE_HOT_RELOAD=true
VITE_OPEN_BROWSER=false