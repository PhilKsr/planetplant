# Production Environment Variables for Frontend
# Used by Vite during build

# API Configuration - relative URLs for production
VITE_API_URL=/api

# WebSocket Configuration - relative URL for production
VITE_WS_URL=

# Feature Flags
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_PWA_DEBUG=false
VITE_LOG_LEVEL=warn

# Production Options
VITE_HOT_RELOAD=false